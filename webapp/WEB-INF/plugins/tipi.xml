<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?><plug-in>
    <name>tipi</name>
    <class>fr.paris.lutece.portal.service.plugin.PluginDefaultImplementation</class>
    <version>1.0.3</version>
    <documentation/>
    <installation/>
    <changes/>
    <user-guide/>
    <description>tipi.plugin.description</description>
    <provider>tipi.plugin.provider</provider>
    <provider-url>http://fr.lutece.paris.fr</provider-url>
    <icon-url>images/admin/skin/feature_default_icon.png</icon-url>
    <copyright>Copyright (c) 2016 Ville de Paris</copyright>
    <db-pool-required>1</db-pool-required>
    
    <core-version-dependency>
        <min-core-version>5.1.0</min-core-version>
        <max-core-version/>	
    </core-version-dependency>

    <freemarker-macro-files>
    </freemarker-macro-files>

    <!-- Admin features parameters -->
    <admin-features>
  
    </admin-features>
    
  	<!-- XPages parameters -->
    <applications>
    </applications>

	<!-- Portlet parameters -->
    <portlets>
    </portlets>

   <!-- Servlet -->
   <servlets>
	  <servlet>
	    <servlet-name>TipiServlet</servlet-name>
	    <servlet-class>fr.paris.lutece.plugins.tipi.web.TipiServlet</servlet-class>
   		<url-pattern>/servlet/plugins/tipi/return</url-pattern>
	  </servlet>
	  
	</servlets>
    
    <!-- Daemons -->
    <daemons>
        <daemon>
         	<daemon-id>tipiDeamon</daemon-id>
            <daemon-name>tipi.dameon.name</daemon-name>
            <daemon-description>tipi.dameon.description</daemon-description>
            <daemon-class>fr.paris.lutece.plugins.tipi.business.TipiTask</daemon-class>
        </daemon>
    </daemons>
</plug-in>